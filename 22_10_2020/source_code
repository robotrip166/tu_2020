ZAD 1:

#include <stdio.h>

int main() {
    int age;
    char info[] = " Zahari Tasev 141220052";
    
    printf("Enter your age: ");
    scanf("%d",&age);
    
    printf("\nYour age is %d", age);
    printf("\n%s",info);
}

