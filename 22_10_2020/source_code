ZAD 2:

#include <stdio.h>
 
int main(void) {
    float velocity_mph;
    float MILES_INTO_KILOMETERS= 1.60934;

    printf("Enter m/ph:");
    scanf("%f", &velocity_mph);

    float velocity_kmph = velocity_mph * MILES_INTO_KILOMETERS;

    printf("%f km/ph", velocity_kmph);
}

