ZAD 3:

#include <stdio.h>
 
int main()
{
    float X=12.0f;
    float Y=6.0f;

    float solution1=((1/((X*X)+1))+(Y*Y/((Y*Y)-1))+(5/((X*X)+(Y*Y))))/(1/((X*X)-(Y*Y)));

    printf("solution 1: %f\n",solution1);

    float a=1/((X*X)+1);
    float a1=Y*Y/((Y*Y)-1);
    float a2=5/((X*X)+(Y*Y));
    float a3 =1/((X*X)-(Y*Y));
    float solution2=(a+a1+a2)/a3;

    printf("solution 2: %f",solution2);
}

